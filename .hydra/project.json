{
  "enabled": 1,
  "hidden": false,
  "description": "GitHub Pull Request Jobset Generator",
  "nixexprinput": "generator",
  "nixexprpath": "jobset/generate.nix",
  "checkinterval": 300,
  "schedulingshares": 100,
  "enableemail": false,
  "emailoverride": "",
  "keepnr": 3,
  "inputs": {
    "generator_config": {
      "type": "git",
      "value": "https://github.com/marigold-dev/tezos-nix.git main",
      "emailresponsible": false
    },
    "generator": {
      "type": "git",
      "value": "https://github.com/DeterminateSystems/hydra-github-jobsets-generator.git make-flake-definition-fixup-flake-uri",
      "emailresponsible": false
    },
    "nixpkgs": {
      "type": "git",
      "value": "https://github.com/NixOS/nixpkgs.git nixos-unstable-small",
      "emailresponsible": false
    },
    "pull_requests": {
      "type": "githubpulls",
      "value": "marigold-dev tezos-nix",
      "emailresponsible": false
    }
  }
}
